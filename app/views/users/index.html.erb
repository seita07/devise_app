<h1>ユーザー一覧</h1>

<%= will_paginate %>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <h2>ユーザーID：<%= user.id%></h2>
      <p>ユーザー名：<%= link_to "#{user.name}", "/users/#{user.id}" %></p>
      <p><strong>email：</strong><%= user.email%></p>
    </li>
  <% end %>
</ul>

<%= will_paginate %>
